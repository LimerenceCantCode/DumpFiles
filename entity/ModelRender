package limerence.AdditionalTraits.render;

import limerence.AdditionalTraits.entity.EntityNatFishHook;
import net.minecraft.client.renderer.entity.Render;
import net.minecraft.client.renderer.entity.RenderManager;
import net.minecraftforge.fml.client.registry.IRenderFactory;

public class RenderNatHook implements IRenderFactory<EntityNatFishHook> {
	
    @Override
    public Render<? super EntityNatFishHook> createRenderFor(RenderManager manager) {
        return new RenderNatFishinghook(manager);
    }

}
