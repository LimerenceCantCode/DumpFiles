    @SubscribeEvent
    public static void entityRegister(RegistryEvent.Register<EntityEntry> event) {
    	int id = 38;
    	
    	EntityEntry NatHook = EntityEntryBuilder.create()
    			.entity(EntityNatFishHook.class)
    			.id(new ResourceLocation("nathook"), id++)
    			.name("nathook")
    			.tracker(64, 3, true)
    			.build();	
    	
    	
    	
    event.getRegistry().register(NatHook);
    
    
    }
}
